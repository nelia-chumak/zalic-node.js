{
  "name": "zalic",
  "version": "1.0.0",
  "engines": {
    "node": ">=16 <17",
    "npm": ">=8 <9"
  },
  "main": "index.js",
  "scripts": {
    "start": "node -r esm index.js",
    "start:dev": "nodemon -r esm index.js",
    "lint": "eslint \"**/*.js\"",
    "lint:fix": "eslint \"**/*.js\" --fix",
    "prettier": "prettier --check \"**/*.js\"",
    "prettier:fix": "prettier --write \"**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,js}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neonilakunyk/zalic-node.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neonilakunyk/zalic-node.js/issues"
  },
  "homepage": "https://github.com/neonilakunyk/zalic-node.js#readme",
  "dependencies": {
    "axios": "0.24.0",
    "dotenv": "10.0.0",
    "esm": "3.2.25",
    "fastify": "3.25.3",
    "fastify-cors": "6.0.2",
    "mongoose": "6.1.4",
    "pino": "7.6.2",
    "pino-pretty": "7.3.0",
    "query-string": "7.0.1"
  },
  "devDependencies": {
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "nodemon": "2.0.15",
    "prettier": "2.5.1"
  }
}
