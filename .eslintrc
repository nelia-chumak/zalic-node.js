{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": { "sourceType": "module" },
  "rules": {
    "prettier/prettier": 2,
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "no-console": 1,
    "max-len": [
      "error",
      {
        "code": 120
      }
    ],
    "no-plusplus": 0,
    "no-multiple-empty-lines": 2,
    "no-var": 2
  }
}
